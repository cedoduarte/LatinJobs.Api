{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Error"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "RolesDB": "Server=localhost\\SQLEXPRESS;Database=LatinJobsProd;Integrated Security=True;TrustServerCertificate=True"
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": true,
    "RealIpHeader": "X-Forwarded-For",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "GET:/api/WeatherForecast/get",
        "Period": "1m",
        "Limit": 5
      },
      {
        "Endpoint": "GET:/api/WeatherForecast/get",
        "Period": "1h",
        "Limit": 100
      }
    ]
  }
}
